= form_for @skill_tree do |f|
  -if @skill_tree.errors.any?
    #error_explanation
      %h2= "#{pluralize(@skill_tree.errors.count, "error")} prohibited this skill_tree from being saved:"
      %ul
        - @skill_tree.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :skill_model
    = f.text_field :skill_model
  .actions
    = f.submit 'Save'
